{
  "name": "nest-puppeteer",
  "description": "Puppeteer (Headless Chrome) provider for Nest.js",
  "license": "MIT",
  "author": "Konstantin Vyatkin <tino@vtkn.io>",
  "homepage": "https://github.com/tinovyatkin/nest-puppeteer#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tinovyatkin/nest-puppeteer.git"
  },
  "bugs": {
    "url": "https://github.com/tinovyatkin/nest-puppeteer/issues"
  },
  "version": "3.0.0",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "lint": "oxlint ./src ./__tests__",
    "lint:fix": "oxlint ./src ./__tests__ --fix",
    "format": "oxfmt --write ./src ./__tests__",
    "format:check": "oxfmt --check ./src ./__tests__",
    "typecheck": "tsgo --noEmit",
    "prepublishOnly": "npm run build",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest --config jest.config.json",
    "test:coverage": "NODE_OPTIONS='--experimental-vm-modules' jest --config jest.config.json --coverage"
  },
  "peerDependencies": {
    "@nestjs/common": ">=10",
    "@nestjs/core": ">=10",
    "puppeteer": ">=22"
  },
  "devDependencies": {
    "@jest/globals": "30.2.0",
    "@nestjs/cli": "11.0.16",
    "@nestjs/common": "11.1.12",
    "@nestjs/core": "11.1.12",
    "@nestjs/platform-express": "11.1.12",
    "@nestjs/schematics": "11.0.5",
    "@nestjs/testing": "11.1.12",
    "@types/express": "5.0.3",
    "@types/node": "22.16.3",
    "@types/supertest": "6.0.3",
    "@typescript/native-preview": "7.0.0-dev.20260118.1",
    "jest": "30.2.0",
    "oxfmt": "0.24.0",
    "oxlint": "1.39.0",
    "puppeteer": "24.35.0",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.2",
    "supertest": "7.2.2",
    "ts-jest": "29.4.6",
    "tsup": "8.5.1"
  },
  "keywords": [
    "browser",
    "headless",
    "nest",
    "nest.js",
    "nestjs",
    "provider",
    "puppeteer"
  ],
  "engines": {
    "node": ">=20"
  }
}
